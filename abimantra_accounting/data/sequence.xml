<odoo>
    <data noupdate="1">
    
        <!-- Sequence untuk Invoice -->
        <record id="sequence_invoice" model="ir.sequence">
            <field name="name">Invoice Sequence</field>
            <field name="code">sequence.account.move.invoice</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequence untuk Bills -->
        <record id="sequence_bills" model="ir.sequence">
            <field name="name">Bills Sequence</field>
            <field name="code">sequence.account.move.bills</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequence untuk Journal Entries -->
        <record id="sequence_journal_entries" model="ir.sequence">
            <field name="name">Journal Entries Sequence</field>
            <field name="code">sequence.account.move.journal</field>
            <field name="padding">5</field>
        </record>
    
    </data>
</odoo>
